.back-link
  color: #c03;
  text-decoration: none;
  font-weight: bold;

.pre-order-header
  text-align: center;

.vehicle-models
  display: grid;
  @media (min-width: 300px)
    grid-template-columns: repeat(1, 1fr)
  @media (min-width: 1024px)
    grid-template-columns: repeat(4, 1fr)
  grid-gap: 20px
